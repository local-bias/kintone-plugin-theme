{
  "name": "kintone-plugin-theme",
  "version": "1.0.0",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "create-ppk": "kintone-plugin-packer --out dist/plugin-dev.zip plugin",
    "move-ppk": "node scripts/move-ppk.ts",
    "init": "run-s merge-manifest:prod create-ppk move-ppk",
    "bundle:dev": "webpack --config webpack.dev.js",
    "bundle:prod": "webpack --config webpack.prod.js",
    "merge-manifest:prod": "ts-node scripts/marge-json.ts prod",
    "build": "run-s  merge-manifest:prod bundle:prod",
    "start": "ts-node scripts/marge-json.ts dev && run-p watch:*",
    "dev": "ts-node scripts/marge-json.ts dev && run-p watch:*",
    "watch": "ts-node scripts/marge-json.ts dev && run-p watch:*",
    "watch:bundle": "webpack --config webpack.dev.js -w",
    "watch:upload": "ts-node scripts/upload.ts",
    "lint": "eslint ./src --cache --ext .js,.ts,.tsx"
  },
  "devDependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@kintone/dts-gen": "6.1.22",
    "@kintone/plugin-packer": "6.0.28",
    "@kintone/plugin-uploader": "7.1.9",
    "@kintone/rest-api-client": "3.3.5",
    "@kintone/webpack-plugin-kintone-plugin": "6.0.28",
    "@lb-ribbit/kintone-secrets": "0.3.0",
    "@lb-ribbit/kintone-xapp": "0.4.0",
    "@mui/icons-material": "5.11.11",
    "@mui/material": "5.11.13",
    "@sentry/react": "7.43.0",
    "@types/luxon": "3.2.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "dotenv": "16.0.3",
    "esbuild-loader": "3.0.1",
    "eslint": "8.36.0",
    "fork-ts-checker-webpack-plugin": "7.3.0",
    "immer": "9.0.19",
    "luxon": "3.2.1",
    "notistack": "2.0.8",
    "npm-run-all": "4.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "recoil": "0.7.7",
    "terser-webpack-plugin": "5.3.7",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-merge": "5.8.0"
  },
  "prettier": {
    "printWidth": 100,
    "arrowParens": "always",
    "singleQuote": true,
    "jsxSingleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "endOfLine": "lf"
  }
}
