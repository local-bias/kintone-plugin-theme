{
  "name": "kintone-plugin-theme",
  "version": "1.0.0",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "create-ppk": "kintone-plugin-packer --out dist/plugin-dev.zip plugin",
    "move-ppk": "node scripts/move-ppk.ts",
    "init": "run-s merge-manifest:prod create-ppk move-ppk",
    "bundle:dev": "webpack --config webpack.dev.js",
    "bundle:prod": "webpack --config webpack.prod.js",
    "merge-manifest:prod": "ts-node scripts/marge-json.ts prod",
    "build": "run-s  merge-manifest:prod bundle:prod",
    "start": "ts-node scripts/marge-json.ts dev && run-p watch:*",
    "dev": "ts-node scripts/marge-json.ts dev && run-p watch:*",
    "watch": "ts-node scripts/marge-json.ts dev && run-p watch:*",
    "watch:bundle": "webpack --config webpack.dev.js -w",
    "watch:upload": "ts-node scripts/upload.ts",
    "lint": "eslint ./src --cache --ext .js,.ts,.tsx"
  },
  "devDependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@kintone/dts-gen": "7.0.3",
    "@kintone/plugin-packer": "7.0.3",
    "@kintone/plugin-uploader": "8.0.0",
    "@kintone/rest-api-client": "3.3.15",
    "@kintone/webpack-plugin-kintone-plugin": "6.0.39",
    "@lb-ribbit/kintone-secrets": "0.3.0",
    "@lb-ribbit/kintone-xapp": "0.4.0",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.13.6",
    "@sentry/react": "7.56.0",
    "@types/luxon": "3.3.0",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "dotenv": "16.3.1",
    "esbuild-loader": "3.0.1",
    "eslint": "8.43.0",
    "fork-ts-checker-webpack-plugin": "8.0.0",
    "immer": "10.0.2",
    "luxon": "3.3.0",
    "notistack": "3.0.1",
    "npm-run-all": "4.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.10",
    "recoil": "0.7.7",
    "terser-webpack-plugin": "5.3.9",
    "ts-node": "10.9.1",
    "typescript": "5.1.3",
    "webpack": "5.88.0",
    "webpack-cli": "5.1.4",
    "webpack-merge": "5.9.0"
  },
  "prettier": {
    "printWidth": 100,
    "arrowParens": "always",
    "singleQuote": true,
    "jsxSingleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "endOfLine": "lf"
  }
}
